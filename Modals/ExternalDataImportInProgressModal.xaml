<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StudentRandomizer.Modals.ExternalDataImportInProgressModal"
             Title="Import danych w toku">
  <Grid
    x:Name="externalDataPage_importInProgressGrid"
    RowDefinitions="*"
    ColumnDefinitions="*">
    <Label
      x:Name="externalDataPage_importInProgressLabel"
      StyleClass="centered-label, full-screen-label, import-export-label"
      Grid.Row="0"
      Grid.Column="0"
      Text="Import danych w toku. Proszę czekać..."
      HorizontalOptions="Center"
      VerticalOptions="Center"
      HorizontalTextAlignment="Center"
      VerticalTextAlignment="Center" />
  </Grid>
</ContentPage>